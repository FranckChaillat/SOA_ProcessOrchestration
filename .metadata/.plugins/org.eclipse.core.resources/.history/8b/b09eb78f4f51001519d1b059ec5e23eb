package processOrchestration

/**
 * @author fra
 */
class Constraint(f: String, func : String, v: String) {
  private val _field = f
  private val _function = func
  private val _value = v
  private var _failed = false
  
  def failed = _failed
  def field = _field
  def function = _function
  def value = _value
  
  
}